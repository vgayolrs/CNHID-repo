<!-- Este esquema básico parte del proyecto CHART con adecuaciones a CNHID -->
<TEI xmlns="http://www.tei-c.org/ns/1.0">
  <teiHeader>
      <fileDesc>
  <!-- Contiene la descripción bibliográfica completa del archivo electrónico -->
         <titleStmt>
            <!-- Información sobre el título de la obra y los responsables del
            contenido -->
            <!-- En orden, a continuación: título de la obra, fuente de
            financiación, responsable principal de la creación del texto
            electrónico y otra información sobre las personas responsables para
            distintos aspectos de la edición -->
            <title>Título</title>
            <funder>Patrocinio</funder>
            <principal>Nombre del investigador principal</principal>
            <respStmt>
               <resp>transcripción</resp>
               <name>Nombre</name>
            </respStmt>
         </titleStmt>
         <publicationStmt>
            <!-- Información sobre la entidad responsable de la publicación -->
            <publisher>El Colegio de Michoacán, A.C.</publisher>
            <pubPlace>Zamora, Michoacán</pubPlace>
            <date>2021</date>
            <idno></idno>
            <distributor>El Colegio de Michoacán, A.C.</distributor>
            <availability><licence>Creative Commons</licence></availability>
         </publicationStmt>
         <sourceDesc>
            <!-- Información sobre la fuente para edición en formato digital -->
            <msDesc>
               <msIdentifier>
                  <country>México</country>
                  <settlement>CDMX</settlement>
                  <repository>Archivo General de la Nación</repository>
                  <idno></idno>
               </msIdentifier>
               <msContents>
                  <summary></summary>
                  <msItem>
                     <locus></locus>
                     <note></note>
                  </msItem>
               </msContents>
							 <!-- estado físico del documento -->
               <physDesc>
                  <objectDesc>
                     <supportDesc>
                        <condition></condition>
                     </supportDesc>
                  </objectDesc>
               </physDesc>
               <history>
							 <!-- historia del documento -->
                  <origin>
                     <origDate></origDate>
                     <origPlace></origPlace>
                  </origin>
               </history>
            </msDesc>
         </sourceDesc>
      </fileDesc>
      <encodingDesc>
			<!-- descripción de la codificación -->
         <projectDesc>
            <p></p>
         </projectDesc>
         <editorialDecl>
            <p></p>
         </editorialDecl>
         <classDecl>
            <taxonomy xml:id="T-CNHID">
               <bibl>Tipología diplomática CNHID, 2021</bibl>
               <!-- Definir, a partir de la lista propuesta, un auto acordado de la Aduiencia de México -->
               <category xml:id="auDel-RlProv">
                  <category xml:id="AudMx-AutAcord">
                     <catDesc>Auto Acordado de la Audiencia de México</catDesc>
                  </category>
               </category>
            </taxonomy>
         </classDecl>
      </encodingDesc>
      <profileDesc>
         <!-- Información no bibliográfica sobre el texto, como las lenguas
         utilizadas, el contexto de producción y los participantes en su
         creación -->
         <langUsage>
            <language ident="es">español</language>
         </langUsage>
         <textClass>
            <catRef/>
         </textClass>
      </profileDesc>
      <revisionDesc>
      <!-- Información sobre las revisiones del archivo electrónico -->
         <change when="2021-05-17">Creación del archivo</change>
      </revisionDesc>
